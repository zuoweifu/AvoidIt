{
  "manifest_version": 2,

  "name": "Avoid It",
  "version": "0.1.0",
  "description": "Avoid the pictures you don't wanna see",
  "options_page": "index.html",
  "permissions": [
          "storage"
        ],
  "content_scripts": [{
    "js": ["jquery-3.3.1.min.js","content.js"],
    "matches": ["<all_urls>"]
  }]

}
